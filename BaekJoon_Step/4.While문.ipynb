   "source": [
    "# import sys\n",
    "# sys.stdin=open(\"./Input.txt\")\n",
    "\n",
    "# N=int(sys.stdin.readline())\n",
    "N=int(input())\n",
    "count=0\n",
    "FirstN=N\n",
    "\n",
    "while True:\n",
    "    count+=1\n",
    "    compareN=10*(N%10)+((N//10)+(N%10))%10\n",
    "    \n",
    "    if FirstN==compareN:\n",
    "        break\n",
    "    N=compareN\n",
    "print(count)"
   ]
  }
