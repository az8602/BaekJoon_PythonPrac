    "import sys\n",
    "sys.stdin=open(\".\\Input.txt\")\n",
    "\n",
    "Nlist=[]\n",
    "n=int(sys.stdin.readline())\n",
    "\n",
    "for _ in range(n):\n",
    "    m=int(sys.stdin.readline())\n",
    "    \n",
    "    if m==0:\n",
    "        Nlist.pop()\n",
    "    else:\n",
    "        Nlist.append(m)\n",
    "        \n",
    "print(sum(Nlist))"
   ]
