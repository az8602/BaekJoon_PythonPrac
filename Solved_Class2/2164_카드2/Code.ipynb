    "import sys\n",
    "from collections import deque\n",
    "sys.stdin=open(\".\\Input.txt\")\n",
    "\n",
    "dq=deque([x for x in range(1,int(sys.stdin.readline())+1)])\n",
    "\n",
    "while len(dq)!=1:\n",
    "    dq.popleft()\n",
    "    dq.append(dq[0])\n",
    "    dq.popleft()\n",
    "print(dq[0])"
   ]
  }
